    "babel-plugin-minify-replace": "^0.5.0",
    "browser-resolve": "^1.11.2",
    "estree-walker": "^1.0.1",
    "json5": "^1.0.1",
    "lodash": "^4.17.21",
    "lru-cache": "^4.1.2",
    "meriyah": "^1.9.7",
    "mz": "^2.7.0",
    "node-fetch": "^2.6.0",
    "npm-package-arg": "^6.0.0",
    "pacote": "^6.0.2",
    "raven": "^2.2.1",
