    "@types/babel__traverse": "^7.0.6",
    "@types/browser-resolve": "^0.0.5",
    "@types/express": "^4.0.37",
    "@types/jest": "^21.1.1",
    "@types/json5": "^0.0.29",
    "@types/lodash": "^4.14.106",
    "@types/lru-cache": "^4.1.0",
    "@types/mz": "^0.0.32",
    "@types/node": "latest",
    "@types/node-fetch": "^2.5.7",
    "@types/npm-package-arg": "^5.1.0",
    "@types/raven": "^2.1.2",
    "@types/recursive-readdir": "^2.2.0",
