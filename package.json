    "express": "^4.16.1",
    "jest": "^21.2.1",
    "nodemon": "^2.0.4",
    "serverless-api-cloudfront": "^0.9.2",
    "serverless-offline": "^3.16.0",
    "serverless-plugin-typescript": "^1.1.3",
    "serverless-plugin-warmup": "^4.8.0",
    "ts-jest": "^21.0.1",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "transform": {
